[32m+[m[32musing System.Collections;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing UnityEngine;[m
[32m+[m[32musing UnityEngine.UI;[m
[32m+[m
[32m+[m[32mpublic class StartButtonScript : MonoBehaviour[m
[32m+[m[32m{[m
[32m+[m[32m    [SerializeField][m
[32m+[m[32m    private Button button;[m
[32m+[m
[32m+[m[32m    [SerializeField][m
[32m+[m[32m    private GameObject alien;[m[41m    [m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[32m    [SerializeField][m
[32m+[m[32m    private GameObject ship;[m[41m    [m
[32m+[m
[32m+[m[32m    [SerializeField][m
[32m+[m[32m    private GameObject tower;[m[41m    [m
[32m+[m
[32m+[m[32m    [SerializeField][m
[32m+[m[32m    private GameObject cannon;[m
[32m+[m
[32m+[m[32m    // Start is called before the first frame update[m
[32m+[m[32m    void Start()[m
[32m+[m[32m    {[m
[32m+[m[32m        button.GetComponent<Button>().onClick.AddListener(() => {[m
[32m+[m[32m            Vector3 towerPositon = tower.transform.position;[m
[32m+[m[32m            Vector3 shipPosition = towerPositon;[m
[32m+[m[32m            towerPositon.x = towerPositon.x + 0.1f;[m
[32m+[m
[32m+[m[32m            shipPosition.x = towerPositon.x + 0.1f;[m
[32m+[m[32m            shipPosition.y = towerPositon.y + 0.1f;[m
[32m+[m
[32m+[m[32m   /*       towerPositon.y = towerPositon.y + 0.05f;[m
[32m+[m[32m            towerPositon.z = towerPositon.z + 0.05f;*/[m
[32m+[m
[32m+[m[32m            GameObject alienObject = Instantiate(alien);[m
[32m+[m[32m            GameObject shipObject = Instantiate(ship);[m
[32m+[m
[32m+[m[32m            alienObject.transform.position = towerPositon;[m
[32m+[m[32m            shipObject.transform.position = shipPosition;[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m            Debug.Log("d");[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Update is called once per frame[m
[32m+[m[32m    void Update()[m
[32m+[m[32m    {[m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m}[m
